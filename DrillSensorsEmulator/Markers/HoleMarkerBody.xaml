<UserControl
    x:Class="DrillSensorsEmulator.Markers.HoleMarkerBody"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:DrillSensorsEmulator.Markers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Width="10"
    Height="10"
    mc:Ignorable="d">
    <Grid>
        <Ellipse
            Name="MarkerView"
            Fill="Black"
            Opacity="1"
            Stroke="White"
            StrokeThickness="1" />
    </Grid>
</UserControl>
